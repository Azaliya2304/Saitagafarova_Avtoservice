<Window x:Class="Саитягафарова_Автосервис.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Саитягафарова_Автосервис"
        mc:Ignorable="d"
        Title="Саитягафарова Азалия" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="/hbc/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        
        <Image Source="/hbc/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Автосервис" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack"  HorizontalAlignment="Right" Margin="5"
                Click="BtnBack_Click"></Button>
        
        
    </Grid>
</Window>
